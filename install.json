{
    "resources": {
        "body": [
            {
                "type": "style",
                "src": "style.css"
            },
            {
                "type": "script",
                "src": "insert.js"
            },
            {
                "type": "script",
                "src": "clipboard.min.js"
            },
            {
                "type": "script",
                "src": "clipboard.js"
            },
            {
                "type": "script",
                "src": "selection-share.js",
                "moduleType": "global",
                "depends": {
                  "github.com/jquery/jquery:jquery.js": "jQuery"
                }
            }
        ]
    },
    "dependencies": {
      "github.com/jquery/jquery": ">= 1.7.0, <= 1.10.2"
    },
    "options": {
        "properties": {
            "hlcolor": {
                "title": "Highlight color",
                "type": "string",
                "format": "color",
                "default": "#F3F315"
            },
            "ttcolor": {
                "title": "Tooltipp color",
                "type": "string",
                "format": "color",
                "default": "#0D1126"
            },
            "tthcolor": {
                "title": "Tooltipp hover color",
                "type": "string",
                "format": "color",
                "default": "#696969"
            },
            "advancedOptionsToggle": {
        "title": "Show advanced options",
        "type": "boolean"
      },
      "advancedOptions": {
        "showIf": "advancedOptionsToggle",
        "title": "Advanced Options",
        "type": "object"
        }
      }
    }
}
